{
  "name": "mock",
  "displayName": "Mock",
  "description": "A VSCode extension",
  "version": "0.0.1",
  "publisher": "your-name",
  "engines": {
    "vscode": "^1.90.0"
  },
  "activationEvents": ["onCommand:mock.start"],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "mock.start",
        "title": "Mock Start"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.90.0",
    "typescript": "^5.4.0",
    "vscode": "^1.1.42"
  }
}
